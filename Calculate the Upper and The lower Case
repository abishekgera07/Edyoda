def string_test(s):
    d={"U":0, "L":0}
    for c in s:
        if c.isupper():
           d["U"]+=1
        elif c.islower():
           d["L"]+=1
        else:
           pass
    print ("Original String : ", s)
    print ("No. of Upper case characters : ", d["U"])
    print ("No. of Lower case Characters : ", d["L"])

string_test('The quick Brown Fox')
