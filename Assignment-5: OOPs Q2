class Calculator:

    def __init__(self,num1,num2):
        self.num1 = num1
        self.num2 = num2
        
    def add(self):
        addition = self.num1 + self.num2
        return addition
        
    def subtract(self):
        subtraction = self.num2 - self.num1
        return subtraction
        
    def multiply(self):
        multiplication = self.num1 * self.num2
        return multiplication
        
    def divide(self):
        division = self.num2 / self.num1
        return division
        

obj = Calculator(10, 94)
print(obj.add())
print(obj.subtract())
print(obj.multiply())
print(obj.divide())
