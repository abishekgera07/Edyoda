class Student:

    def setName(self,name):
        self.__name = name
        
    def getName(self):
        return self.__name
        
    def setRollNumber(self,rollNumber):
        self.__rollNumber = rollNumber
        
    def getRollNumber(self):
        return self.__rollNumber

S1 = Student()
S1.setName("Abishek")
S1.setRollNumber(23)
print(S1.getName())
print(S1.getRollNumber())
